#include<stdio.h>
int main(){
  int num[],i,limit,number;
  for(i=0;i<limit;i++)
  {
  printf("\nenter the elements");
  scanf("%d",&num[i]);
  }
  printf("enter the elements to be searched");
  scanf("%d",&number);
  for(i=0;i<limit;i++){
  if(num[i]==number){
  printf("the position is:");
  printf("%d",i+1);
  }
  }
  return 0;
  }
